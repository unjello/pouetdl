{
  "name": "pouetdl",
  "version": "1.0.0",
  "description": "A command-line tool to fetch pouet.net productions metainformation and store as CSV",
  "main": "bin/pouetdl.js",
  "bin": "bin/pouetdl.js",
  "preferGlobal": true,
  "author": "Andrzej Lichnerowicz <andrzej@lichnerowicz.pl>",
  "license": "GPL-3.0",
  "engine": {
    "node": ">=8.0"
  },
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "build:watch": "babel src --watch -d dist --source-maps",
    "pretty": "prettier --write --single-quote \"{src,test}/**/*.js\" ",
    "lint": "eslint ./src ./test"
  },
  "dependencies": {
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "lodash": "^4.17.4",
    "node-fetch": "^2.0.0-alpha.9",
    "winston": "^3.0.0-rc1",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "jest-cli": "^21.2.1",
    "prettier": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unjello/pouetdl"
  },
  "bugs": {
    "url": "https://github.com/unjello/pouetdl/issues"
  }
}
